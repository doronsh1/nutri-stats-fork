{
  "name": "nutristats",
  "version": "1.3.0",
  "description": "NutriStats - Comprehensive nutrition tracking and analytics application",
  "main": "server.js",
  "scripts": {
    "build": "mkdir -p dist && cp -r server.js src scripts public package.json .env dist",
    
    "start": "node server.js",
    "start:test": "NODE_ENV=test PORT=8080 node server.js",
    "dev": "nodemon server.js",
    "test:e2e": "cd e2e-tests && npm test",
    "test:e2e:headed": "cd e2e-tests && npm run test:headed",
    "test:e2e:ci": "cd e2e-tests && AUTH_STRATEGY=jwt npm test",
    "migrate": "node scripts/migrate.js",
    "migrate:init": "node scripts/migrate.js init",
    "migrate:foods": "node scripts/migrate.js foods",
    "migrate:users": "node scripts/migrate.js users",
    "migrate:user-foods": "node scripts/migrate-user-foods.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "canvas": "^3.1.2",
    "csv-parse": "^6.0.0",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.5",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
